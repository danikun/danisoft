<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="contactsPage" class="org.danisoft.ui.pages.ContactsPage" scope="prototype">
		<constructor-arg name="_contactsService" ref="contactsService"/>
	</bean>
	
	<bean id="personDetailsComponent" class="org.danisoft.ui.custom.PersonDetailsComponent" scope="prototype">
		<property name="contactsService" ref="contactsService"/>
	</bean>
</beans>
